{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "wing-review/wing-review",
	"title": "Wing Review",
	"category": "widgets",
	"icon": "star-filled",
	"description": "Displays a wing location review with ratings",
	"keywords": ["review", "wings", "rating", "restaurant"],
	"textdomain": "wing-review",
	"supports": {
		"html": false
	},
	"attributes": {
		"reviewerName": {
			"type": "string",
			"default": ""
		},
		"reviewerEmail": {
			"type": "string",
			"default": ""
		},
		"rating": {
			"type": "number",
			"default": 0
		},
		"sauceRating": {
			"type": "number",
			"default": 0
		},
		"crispinessRating": {
			"type": "number",
			"default": 0
		},
		"reviewText": {
			"type": "string",
			"default": ""
		},
		"timestamp": {
			"type": "string",
			"default": ""
		},
		"saucesTried": {
			"type": "string",
			"default": ""
		},
		"wingCount": {
			"type": "number",
			"default": 0
		},
		"totalPrice": {
			"type": "number",
			"default": 0
		}
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./editor.css",
	"style": "file:./style.css"
}
